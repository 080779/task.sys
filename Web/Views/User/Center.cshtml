<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
    <title>个人中心</title>
    <link rel="stylesheet" type="text/css" href="/Task/css/common.css" />
    <link rel="stylesheet" type="text/css" href="/Task/css/main.css" />
    <link rel="stylesheet" href="/Task/css/style.css">
    <script type="text/javascript" charset="utf-8" src="/Task/js/jquery-1.11.1.min.js"></script>
    <script type="text/javascript" charset="utf-8" src="/Task/js/index.js"></script>
</head>
<body class="grey">
    <div class="top">
        <div class="wrap-white">
            <i class="itemslist"><a href="#"></a></i>
            <div class="ptitle-white">个人中心</div>
            <!--<i class="searchi"><a href="#"></a></i>-->
        </div>
    </div>
    <div class="itemslist-bg">
        <div class="itemslist-panel">
            <p id="index_page"><a href="/task/index">首页</a></p>
            <p id="index_mytask"><a href="/user/mytask"> 我的任务</a></p>
            <p id="index_user"><a href="/user/center">个人中心</a> </p>
        </div>
    </div>
    <!--头像-->
    <div class="user_bady_img">
        <div class="user_img_list">
            <div class="user_content_img">
                <img src="/Task/images/userimg.png" />
            </div>
            <div class="user_content_text">段鱼</div>
            <div class="user_content_id">账户ID：SDYTH</div>
        </div>
    </div>
    <!--账户-->
    <div class="user_content_num">
        <div class="user_content_mon">
            <div class="content_mon_img">
                <img src="/Task/images/money.png" />
            </div>
            <div class="content_mon_text">账户余额（元）</div>
        </div>
        <div class="user_content_money">
            <div class="content_money_num">123456.00</div>
            <div class="content_money_right">
                <div class="money_right_rcond">提现记录</div>
                <a href="/user/takecash" class="money_right_apply">提现</a>
            </div>
        </div>
    </div>
    <!--记录-->
    <div class="user_list_content">
        <!--收入记录-->
        <div class="list_content_rcond">
            <div class="content_rcond_text">
                <div class="rcond_text_img"><img src="/Task/images/imfgreen.png" /></div>
                <div class="rcond_text">收入记录</div>
            </div>
            <div class="rcond_text_img"><img src="/Task/images/right_img.png" /></div>
        </div>
        <!--任务记录-->
        <div class="list_content_rcond">
            <div class="content_rcond_text">
                <div class="rcond_text_img"><img src="/Task/images/imfblue.png"></div>
                <div class="rcond_text">任务记录</div>
            </div>
            <div class="rcond_text_img"><img src="/Task/images/right_img.png" /></div>
        </div>
        <!--联系客服-->
        <div class="list_content_rcond">
            <div class="content_rcond_text">
                <div class="rcond_text_img"><img src="/Task/images/conred.png"></div>
                <div class="rcond_text">联系客服</div>
            </div>
            <div class="rcond_text_img"><img src="/Task/images/right_img.png" /></div>
        </div>
    </div>
    <script type="text/javascript" charset="utf-8" src="/Task/js/flexible.js"></script>
</body>
</html>